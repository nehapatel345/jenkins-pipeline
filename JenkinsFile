pipeline {
  agent any
  
  
  stages {
    stage('Checkout') {
      steps {
        git branch: 'main', url: 'https://github.com/pavanpalveproject/jenkins-pipeline'
      }
    }
    
    
    
    stage('Terraform Init') {
      steps {
        sh 'terraform init'
      }
    }
    
    
    
    
  }
}
